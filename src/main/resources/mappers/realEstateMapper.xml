<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.marondal.test.lesson02.dao.realEstateDAO">
 <select id="selectRealEstate" resultType="com.marondal.test.lesson03.modal.realEstate">
 
 	SELECT 
 		`id`
 		,`realtorId`
 		,`address`
 		,`area`
 		,`type`
 		,`price`
 		,`rentPrice`
 		,`createdAt`
 		,`updatedAt`
 	FROM
 		`real_estate`
 	WHERE
 		`id`=#{id}
 
 
 </select>
 
 
 </mapper>